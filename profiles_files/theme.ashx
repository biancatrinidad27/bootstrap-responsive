/* New Module Player overwrites */

/* CSS for customers to brand the player */

/* top and side nav background */
.navbarBkg {background-color:#ffffff;}

/* Top nav text / color Side nav header */
.topNavText, .topNavText:hover, .topNavText:visited, .sideNavHeader, .m-nav-btn {color:#1bbd9d;}

.m-nav-btn {background: #eaeaea ;}

a.m-nav-item:hover .m-nav-btn{
    background-color: #92d400 ;
    background: #92d400 ;
    color: #ffffff;
}

.navbar-header .btn-nav {
  background-color: #eaeaea ;
  border-color: #eaeaea ;
}

.navbar-default .btn-nav .icon-bar {
  background-color: #1bbd9d;
}



#sidebar-wrapper .container .nav-pills > li.active > a,
#sidebar-wrapper .container .nav-pills > li.active > a:hover,
#sidebar-wrapper .container .nav-pills > li.active > a:focus {
  color: #1bbd9d;
  background-color: #eaeaea ;
}

#sidebar-wrapper .container .nav-pills > li > a,
#sidebar-wrapper .container .nav-pills > li > a:hover,
#sidebar-wrapper .container .nav-pills > li > a:focus {
  color: #ffffff;
  background-color: #eaeaea ;
}

/* Side nav text links */ 
.sidebarText a, .sidebarText {color:#1bbd9d;}

.sidebarText a:hover {
    color:#eaeaea ;
    text-decoration:none;
}

.sidemenuBkg {background-color:#eaeaea ;}

#sidebar-wrapper .container .nav-pills 
{
    background-color:#eaeaea ;
}

/* Highlighted color of active module */
.activeModuleColor {color:#92d400  !important;}

/* Circle icon complete or in progress */
.moduleListIconComplete, .moduleListIconInProgressL {background-color:#92d400 ;}

/* Circle icon not started */
.moduleIconNotStarted {background-color:#1bbd9d;}

.moduleListLine {background-color:#1bbd9d;}

.additionalReferencesList a,  .additionalReferencesList p, .additionalReferencesList i {color:#1bbd9d;}

.additionalReferencesList {
    color:#1bbd9d;
    border-bottom:1px solid #1bbd9d;
}
.topic-menu li:hover
{
    border-left:4px solid #ffffff;
}